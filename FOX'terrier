# Initialize FOX miner with hardware specifications
fox_miner = FoxMiner(cpu="HighPerformanceCPU", gpus=["GPU1", "GPU2"], ram="HighSpeedRAM", storage="FastSSD", cooling_system="EfficientCooling")

